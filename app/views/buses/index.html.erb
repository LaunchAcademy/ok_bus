<h1>Buses</h1>

<% @buses.each do |bus| %>
  <ul>
    <li>
      <%= link_to bus.number, bus_path(bus) %> - <%= bus.inbound %> - <%= bus.outbound %>
    </li>
  </ul>
<% end %>

<% @users.each do |user| %>
  <ul>
    <li>
      <%= user.username %> - <img src ="<%= user.profile_photo %>">
    </li>
  </ul>
<% end %>
